<% @wel = Welcome.all %>
<% @wel.each do |w| %>
  <div class="login-box">
    <div class="login-logo">
      <a><b><%= w.nombreEmpresa %> CRM</b></a>
    </div>
    <!-- /.login-logo -->
    <div class="login-box-body">
      <p class="login-box-msg">Digite sus datos para iniciar sesión</p>
  
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-group has-feedback">
            <%= f.text_field :email, :class=>"form-control", placeholder: "Usuario" %>
            <i class="fa fa-user form-control-feedback"></i>
        </div>
        <div class="form-group has-feedback">
          <%= f.password_field :password, autocomplete: "off", :class=>"form-control", placeholder: "Contraseña" %>
            <i class="fa fa-lock form-control-feedback"></i>
        </div>
        <div class="row">
          <div class="col-xs-8">
            <div class="checkbox icheck">
              <label>
                <input type="checkbox"> Recordarme
              </label>
            </div>
          </div>
          <!-- /.col -->
          <div class="col-xs-4">
            <button type="submit" class="btn btn-primary btn-block btn-flat">Ingresar</button>
          </div>
          <!-- /.col -->
        </div>
      <% end %>
      
      <hr>
      
    </div>
    <!-- /.login-box-body -->
  </div>
  <!-- /.login-box -->

<%end%>

<script>
  $(function () {
    $('input').iCheck({
      checkboxClass: 'icheckbox_square-blue',
      radioClass: 'iradio_square-blue',
      increaseArea: '20%' // optional
    });
  });
</script>