<p id="notice"><%= notice %></p>

<h1>Listing Proveedors</h1>

<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Nombre Cliente</th>
      <th>Telefono</th>
      <th>NIT</th>
      <th>Nombre Empresa</th>
      <th>Telefono</th>
      <th>Pagina Web</th>
      <th>Cuenta Contable</th>
      <th>Inicio Contrato</th>
      <th>Fin Contrato</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
        <td><%= proveedor.cedula %></td>
        <td><%= proveedor.personaNombre %></td>
        <td><%= proveedor.personaTelefono %></td>
        <td><%= proveedor.nit %></td>
        <td><%= proveedor.empresaNombre %></td>
        <td><%= proveedor.empresaTelefono %></td>
        <td><%= proveedor.paginaWeb %></td>
        <td><%= proveedor.cuentaContable %></td>
        <td><%= proveedor.fechaInicio %></td>
        <td><%= proveedor.fechaFin %></td>
        <td><%= link_to 'Show', proveedor %></td>
        <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
        <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveedor', new_proveedor_path %>
