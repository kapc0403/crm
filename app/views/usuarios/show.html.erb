<div class="panel panel-primary">
  <div class="panel-heading">
    <h1> <%= @user.nombre %></h1>
  </div>
  <div class="panel-body">
    <d1>
      <dt class="text-info">Email:</dt>
      <dd><%= @user.email %></dd>
      <dt class="text-info">Usuario:</dt>
      <dd><%= @user.nombreUsuario %></dd>
      <dt class="text-info">Rol:</dt>
      <dd><%= @user.rol %></dd>
    </d1>
  </div>
</div>

<div class="actions">
  <a><%= link_to 'Volver', :back, :class=>" fa fa-arrow-circle-left fa-2x btn btn-primary" %></a>
</div>
