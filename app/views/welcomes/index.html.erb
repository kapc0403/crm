<p id="notice"><%= notice %></p>

<h1>Listing Welcomes</h1>

<table>
  <thead>
    <tr>
      <th>Nombreempresa</th>
      <th>Moneda</th>
      <th>Nombreencargado</th>
      <th>Telencargado</th>
      <th>Telempresa</th>
      <th>Direccionempresa</th>
      <th>Correoencargado</th>
      <th>Telempresa</th>
      <th>Direccionempresa</th>
      <th>Correoencargado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @welcomes.each do |welcome| %>
      <tr>
        <td><%= welcome.nombreEmpresa %></td>
        <td><%= welcome.moneda %></td>
        <td><%= welcome.nombreEncargado %></td>
        <td><%= welcome.telEncargado %></td>
        <td><%= welcome.telEmpresa %></td>
        <td><%= welcome.direccionEmpresa %></td>
        <td><%= welcome.correoEncargado %></td>
        <td><%= link_to 'Show', welcome %></td>
        <td><%= link_to 'Edit', edit_welcome_path(welcome) %></td>
        <td><%= link_to 'Destroy', welcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Welcome', new_welcome_path %>
